{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "../../src",
    "outDir": "./dist",
    "declaration": true,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "composite": false,
    "sourceMap": false,
    "noEmit": false,
    "allowImportingTsExtensions": false
  },
  "include": [
    "../../src/engine/**/*"
  ],
  "exclude": [
    "../../src/core/**/*",
    "../../src/game/**/*",
    "../../src/scripts/**/*"
  ]
}
